font<>:fonts {
	font:compacta { Compacta Std Regular.otf }
	font:univers { Univers-light-normal.ttf }
}
class<>:classes {
	class:deployment-pane {
		mode:default {
			int:height { 768 }
			int:width { 128 }
			int[]:padding { 14 14 14 14 }
			color[]:background-color { #000000 #000000 #000000 #000000 }
		}
	}
	class:deployment-select {
		mode:default {
			int:height { 740 }
			int:width { 100 }
			color[]:background-color { #444444 #444444 #444444 #444444 }
		}
	}
	class:deployment-selection-option {
		mode:default {
			int:height { 100 }
			int:width { 100 }
			int[]:margin { 0 3 0 3 }
			color[]:background-color { #888888 #888888 #888888 #888888 }
		}
		mode:selected {
			color[]:background-color { #AAAA88 #AAAA88 #AAAA88 #AAAA88 }
		}
	}
	class:attack-pane {
		mode:default {
			int:height { 512 }
			int:width { 512 }
			color[]:background-color { #000000 #000000 #000000 #000000 }
		}
	}
	class:attack-text-box {
		mode:default {
			int:height { 480 }
			int:width { 480 }
			!font:font-face { fonts.compacta }
			color:font-color { #FFFFFF }
			int:font-size { 32 }
			int[]:padding { 16 16 16 16 }
			int[]:margin { 16 16 16 16 }
		}
	}
	class:large-button {
		mode:default {
			int:height { 64 }
			int:width { 128 }
			!font:font-face { fonts.compacta }
			color:font-color { #FFFFFF }
			int:font-size { 32 }
			color[]:background-color { #000000 #000000 #000000 #000000 }
			int[]:padding { 32 10 32 10 }
		}
		mode:hover {
			color[]:background-color { #BCBC00 #BCBC00 #BCBC00 #BCBC00 }
		}
	}
}